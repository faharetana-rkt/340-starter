<% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
        res.redirect('/')
    } %>
<%- messages() %>
<% if (errors) { %>
    <ul class="notice">
   <% errors.array().forEach(error => { %>
     <li><%= error.msg %></li>
  <%  }) %>
   </ul>
  <% } %>
<div class="management">  
<a href="/inv/addclass" >Add New Classification</a>
<a href="/inv/addinv">Add New Vehicle</a>
<h2>Manage Inventory</h2>
<p>Select a classification to show inventory items.</p>
<%- selectClassification %>
<table id="inventoryDisplay"></table>
<noscript>Javascript must be enabled to use this page.</noscript>
</div>
<script src="../../js/inventory.js"></script>

